/* CSS Document */
/*************************************
*		import css
**************************************/
@import url(reset.css);
@import url(module.css);
@import url(waterpage.css);
@import url(../calendar/popCalendar.css);
@media print {
@import url(print.css);
}
/*************************************
*		Css Layout
**************************************/
html, body {
  background: #fff;
  font: normal 12px  Arial, Helvetica, sans-serif, "新細明體";
}

#wrapper {
  /*height: 100%;*/
  height: auto !important;
  /*min-height: 100%;*/
}

#header {
  background: #ba6f19;
  padding: 10px;
}

#pageBody, .pageBody_wp {
  overflow: hidden;
  /* _overflow:auot;*/
  /*min-height:$body_min-height;*/
  height: 100% !important;
  /*height: $body_min-height;*/
  height: 100%;
}

/* IE6 Fix */
* html div#pageBody, * html .pageBody_wp {
  overflow: visible;
}

#content {
  margin: 0 0 0 200px;
  _margin: 0 0 0 80px;
  /* IE6 Fix */
  zoom: 1;
}
#content .content_wp {
  width: 100%;
  _width: auto;
  float: left;
  _float: none;
  background: #06F;
  padding: 0 10px 0 0;
}

#sideLeft {
  width: 200px;
  float: left;
  background: #393;
  padding: 0px;
}

#footer {
  background: #ba6f19;
  padding: 10px;
}

/*************************************
*		#wrapper
**************************************/
#wrapper {
  background: white url(../img/layout/header_bg.gif) no-repeat right top;
}

/*************************************
*		#header
**************************************/
#header {
  padding: 0;
  background: none;
}
#header .header_wp {
  zoom: 1;
  padding: 9px 14px 0;
}
#header h1 {
  margin: 0 0 10px 2px;
  float: left;
}

/*		.nav
------------------------------------ */
.nav {
  margin: 0 0 5px;
  float: right;
}
.nav li {
  float: left;
  padding: 0 0 0 10px;
}
.nav li a {
  display: inline-block;
  color: #145836;
  padding: 2px 3px 1px;
  padding: 3px 3px 0px \9;
  /*border:1px #71a5cc solid;*/
}
.nav li a:hover {
  text-decoration: underline;
  			    /*background:#ffe640;
              	border:1px #006600 solid;*/
}
.nav .lang {
  margin: 0;
  _font-size: 11px;
  border: 1px #009048 solid;
}
.nav .btn_logout {
  color: #fff;
  font-weight: bold;
  padding: 1px 10px 1px 25px;
  padding: 3px 10px 0px 25px \9;
  border: 1px #009048 solid;
  background: url(../img/btn/btn_logout.gif) repeat-x left -1px;
}
.nav .btn_logout:hover {
  text-decoration: none;
  background: url(../img/btn/btn_logout_hover.gif) repeat-x left -1px;
}

/*		.userInfo
------------------------------------ */
.userInfo {
  margin: 0 0 0px;
  white-space: nowrap;
  color: #145836;
  clear: right;
  float: right;
}
.userInfo .now {
  color: #c5802f;
}

/*		commonFunc
---------------------------------*/
.commonFunc {
  color: #000;
  float: left;
  display: inline-block;
  border: 1px #988840 solid;
  padding: 2px 2px 2px 18px;
  background: #f9f3b2 url(../img/btn/commonFunc.gif) no-repeat 2px center;
  _display: inline;
}
.commonFunc select {
  font-size: 10px;
  height: 18px;
}

/*		Account
---------------------------------*/
.Account {
  color: #125738;
  display: inline-block;
  padding: 0 2px 0 18px;
  background: url(../img/btn/changeAccount.gif) no-repeat 2px center;
  _display: inline;
  _zoom: 1;
}

.Account select {
  font-size: 10px;
  height: 18px;
}

/*		page_index  單元首頁
---------------------------------*/
.page_index{margin:5px 0px 0px;}
.page_index .pic{padding:25px 750px 25px 0;margin:0;background:url(../img/layout/pageindex_01.gif) no-repeat left bottom;}
.page_index .page_cont{
	padding:21px 21px 31px 21px;
	border-top:2px #63a524 solid;
	border-right:1px #b7b7b7 solid;
	border-left:1px #b7b7b7 solid;
	background:url(../img/layout/pageindex_02.gif) repeat-x left top;}
.page_index .hd{font-size:15px;color:#427038;font-weight:bold;}
.page_index .txt1{font-size:12px;color:#b88a28; padding:10px 0 8px 0;line-height:1.4em;}
.page_index .line{border-top:1px #999 dotted;padding:8px 0 5px 0;}
.page_index .table1 td{padding:5px 0;vertical-align:top;line-height:1.4em;color:#505050;}
.page_index .table1 .tit{font-size:13px;color:#63a524;font-weight:bold;}
.page_index .table1 li{list-style:decimal;margin:0 0 0 25px;}
.page_btm{padding:2px;margin:0 0 10px 0;background:url(../img/msg/msg2.gif) repeat-x left top;}
.arrow{padding:5px 0;margin:2px 0 0 0;background:url(../img/icon/icon_arrow.gif) repeat-x;width:10px;}

