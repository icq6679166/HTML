/*		jmask浮動遮罩
------------------------------------ */
.mask_tit {
  color: #fff;
  margin: 0 0 5px;
  padding: 5px 3px 3px 20px;
  background: #06944a url(../img/layout/mask_tit.gif) no-repeat 5px 8px;
}

.mask {
  position: absolute;
  left: 99999px;
  top: 99999px;
  
}

#js_overLayer {
  display: none;
  background: #000;
  width: 100%;
  top: 0;
  left: 0;
  position: fixed;
  _position: absolute;
  filter: alpha(opacity=70);
  -moz-opacity: 0.7;
  opacity: 0.7;
  z-index: 99;
}

#js_container {
  background: #fff;
  width: 300px;
  height: 300px;
  z-index: 100;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-top: -150px;
  margin-left: -150px;
  border: 3px #666 solid;
  display: none;
  overflow: scroll;
}

* html #js_container {
  position: absolute;
}

#hiddenContent {
  display: none;
}

#jmask_tit {
  background: #eee;
  padding: 5px;
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 112;
  display: none;
}

